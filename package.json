{
  "name": "elektrum_frontend",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm mix-manifest.json",
    "watch": "cross-env NODE_ENV=development webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js --watch && rm mix-manifest.json",
    "build": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js && rm mix-manifest.json",
    "start": "serve"
  },
  "dependencies": {
    "backbone": "^1.3.3",
    "bootbox": "^4.4.0",
    "bootstrap": "^3.3.7",
    "bootstrap-datepicker": "^1.6.4",
    "bootstrap-tokenfield": "^0.12.0",
    "datatables.net-bs": "^1.10.13",
    "fancybox": "^3.0.0",
    "font-awesome": "^4.7.0",
    "jquery": "1.11.0",
    "jquery-form": "^4.2.0",
    "jquery-masked-input": "^0.1.2",
    "moment": "^2.18.1",
    "select2": "^4.0.3"
  },
  "devDependencies": {
    "cross-env": "^3.2.4",
    "laravel-mix": "^0.9.2",
    "serve": "^5.1.1"
  }
}
